{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/",
      "dest": "/login.html"
    },
    {
      "src": "/hello",
      "dest": "/hello.html"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/$1"
    }
  ]
}